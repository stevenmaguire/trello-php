{
    "name" : "stevenmaguire/trello-php",
    "description" : "PHP wrapper for Trello API",
    "keywords" : ["trello","php","client","wrapper","trello gold"],
    "homepage" : "http://github.com/stevenmaguire/trello-php",
    "license" : "MIT",
    "authors" : [
        {
            "name" : "Steven Maguire",
            "email" : "stevenmaguire@gmail.com",
            "homepage" : "http://stevenmaguire.com",
            "role" : "Developer"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.0 || ^7.0",
        "league/oauth1-client": "^1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "0.9.*@dev",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Stevenmaguire\\Services\\Trello\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stevenmaguire\\Services\\Trello\\Tests\\": "tests/"
        },
        "classmap": [
            "src/",
            "tests/"
        ]
    }
}
